{
  "flutter": {
    "platforms": {
      "android": {
        "default": {
          "projectId": "production-tracker-top",
          "appId": "1:808369467122:android:82e9e1a6992ff4bd412a6b",
          "fileOutput": "android/app/google-services.json"
        }
      },
      "dart": {
        "lib/firebase_options.dart": {
          "projectId": "production-tracker-top",
          "configurations": {
            "android": "1:808369467122:android:82e9e1a6992ff4bd412a6b",
            "ios": "1:808369467122:ios:bdabf1bdbe1b5292412a6b",
            "macos": "1:808369467122:ios:bdabf1bdbe1b5292412a6b",
            "web": "1:808369467122:web:0b4830ce3ea79d7d412a6b",
            "windows": "1:808369467122:web:8c2158658670431e412a6b"
          }
        }
      }
    }
  },
  "hosting": {
    "public": "build/web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "**",
        "headers": [
          {
            "key": "Cross-Origin-Opener-Policy",
            "value": "same-origin-allow-popups"
          }
        ]
      }
    ]
  },
  "firestore": {
    "indexes": "firestore.indexes.json"
  },
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "disallowLegacyRuntimeConfig": true,
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ],
      "predeploy": [
        "npm --prefix \"$RESOURCE_DIR\" run lint"
      ]
    }
  ]
}
